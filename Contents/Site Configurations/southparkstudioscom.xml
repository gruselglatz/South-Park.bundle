<?xml version="1.0" encoding="UTF-8"?>
<site site="http://www.southparkstudios.com"
	plugin="http://media.mtvnservices.com.*southpark"
	initialState="playing"
	version="2.0">

	<crop x="0" y="0" width="0" height="0" />

	<!-- PLAYING -->
	<state name="playing">
		<event>
			<condition>
				<command name="pause" />
			</condition>
			<action>
				<!-- Nothing to do -->
			</action>
		</event>
	</state>

	<!-- PAUSED -->
	<state name="paused">
		<event>
			<condition>
				<command name="play" />
			</condition>
			<action>
				<!-- Nothing to do -->
			</action>
		</event>
	</state>
</site>